
	c = a + b
	
	class Node {
		private Node left;
		private int data;
		private Node right;
		static int x;			//class variable
		
		Node(Node left, int data, Node right) {
			this.left = left;
			this.data = data;
			this.right = right;
		}
		
		setLeftChild(Node n) {
			this.left = n;
		}
		
		public static void main(String[] args) {
			Node n = new Node(null, 10, null);
			Node n1 = new Node(null, 20, null);
			
			n.setLeftChild(n1);
		}
	}
	

	Original Program
	
	
	Characteristics of a good program:
		- Efficient
		- Robust
		- Scalable
		- Testable
	
	
	void add()	//Not Testable 
	{
	
	}
	
	int add(int a, int b)	//Testable
	{
		return a - b;
	}
	
	
	if(add(10, 20) == 30)	//Assertion
	{
		echo "Test passed!";
	}
	
	
